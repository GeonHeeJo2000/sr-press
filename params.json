{
    "xgboost": {
        "ModelConfig": {
        "n_estimators": 200,
        "max_depth": 5,
        "seed": 0,
        "enable_categorical": false,
        "n_jobs": -1,
        "tree_method": "hist",
        "device": "cuda",
        "verbosity": 0,
        "eval_metric": "logloss"
        }
    },

    "symbolic_regression": {
        "ModelConfig": {
            "population_size": 3000,
            "generations": 100,
            "tournament_size": 30,
            "stopping_criteria": 0.2,
            "const_range": [-1, 1],
            "parsimony_coefficient": 0.001,
            "p_crossover": 0.7,
            "p_subtree_mutation": 0.1,
            "p_hoist_mutation": 0.05,
            "p_point_mutation": 0.1,
            "function_set": ["add", "mul", "sub", "div", "abs", "sqrt", "sin", "tan", "cos", "max", "min"],
            "verbose": 1,
            "random_state": 1,
            "n_jobs": -1,
            "init_depth": [2, 6],
            "max_samples": 0.9
        }
    },

    "soccermap": {
        "DataConfig": {
            "batch_size": 16,
            "num_workers": 8,
            "pin_memory": true
        },
        "TrainerConfig": {
            "max_epochs": 200,
            "min_epochs": 100,
            "gradient_clip_val": 100,
            "devices": 1,
            "accelerator": "auto"
        },
        "OptimizerConfig": {
            "optimizer_params": {
                "lr": 1e-4,
                "weight_decay": 1e-8
            }
        },
        "ModelConfig": {
            "in_channels": 8
        },

        "EarlyStopConfig": {
            "patience": 20,
            "min_delta": 1e-4,
            "mode": "min",
            "strict": false,
            "verbose": true
        },
        "ModelCheckpoint": {
            "save_top_k": 1,
            "verbose": true,
            "mode": "min"
        }
    }
}
